        <value>(function execute(inputs, outputs) {&#13;
&#13;
    // Assuming you're working within a workflow&#13;
&#13;
    var customTableDef = {&#13;
        &#13;
        tableName: 'u_servicenow_table_count',&#13;
        tableLabel:"ServiceNow Table Count",&#13;
        tableExtension:"sys_import_set_row",&#13;
        fieldList:[&#13;
            {&#13;
                usePrefix:true,&#13;
                name:"u_record_import_error",&#13;
                label:"Record Import Error",&#13;
                type:"string",&#13;
                maxLength:"4000"&#13;
            },{&#13;
                usePrefix:true,&#13;
                name:"u_sntablecount",&#13;
                label:"snTableCount",&#13;
                type:"integer"&#13;
            },{&#13;
                usePrefix:true,&#13;
                name:"u_sntablename",&#13;
                label:"snTableName",&#13;
                type:"string",&#13;
                maxLength: "80"&#13;
            },{&#13;
                usePrefix:true,&#13;
                name:"u_sntablebatch",&#13;
                label:"snTableBatch",&#13;
                type:"string",&#13;
                maxLength: "40"&#13;
            }&#13;
        ]&#13;
    };&#13;
    &#13;
    var createCustomTable = new global.CreateCustomTable();&#13;
&#13;
	if(!createCustomTable.tableExists(customTableDef.tableName)) {&#13;
&#13;
        var result = createCustomTable.createTable(customTableDef);&#13;
    }&#13;
&#13;
    var recordsProcessed = 0;&#13;
    var snTableBatch = gs.generateGUID();&#13;
&#13;
    var tableList = new GlideRecord('sys_db_object');&#13;
    tableList.orderBy('name');&#13;
    tableList.query(); // Retrieves all records&#13;
&#13;
    var dataImportTable = new GlideRecord(customTableDef.tableName); // Replace 'your_table_name' with your custom table name&#13;
    dataImportTable.setWorkflow(false);&#13;
&#13;
    while (tableList.next()) {&#13;
&#13;
        recordsProcessed ++;&#13;
        // Insert results into your data import table (replace with actual table name)&#13;
&#13;
        try {&#13;
            &#13;
            var snTableName = tableList.getValue('name'); // Get the name of the table&#13;
&#13;
            //gs.info('************** Now processing table: ' + snTableName + ', batch: ' + snTableBatch);&#13;
            &#13;
            var agg = new GlideAggregate(snTableName);&#13;
            agg.addAggregate('COUNT');&#13;
            agg.query();&#13;
&#13;
            //gs.info('************** Counted table: ' + snTableName + ', batch: ' + snTableBatch);&#13;
&#13;
            if (agg.next()) {&#13;
&#13;
                dataImportTable.initialize();&#13;
                dataImportTable.setWorkflow(false);&#13;
                &#13;
                var snTableRecCount = agg.getAggregate('COUNT');&#13;
&#13;
                // Log the count in the system log&#13;
&#13;
                //gs.info('************** Table ' + snTableName + ' has ' + snTableRecCount + ' records, batch: ' + snTableBatch);&#13;
&#13;
                dataImportTable.setValue('u_sntablebatch', snTableBatch);&#13;
                dataImportTable.setValue('u_sntablename', snTableName);&#13;
                dataImportTable.setValue('u_sntablecount', snTableRecCount);&#13;
&#13;
                dataImportTable.insert();&#13;
            }&#13;
            &#13;
        } catch(e) {&#13;
            gs.error('*********** GetRowCounts Error: ' + e);&#13;
&#13;
            dataImportTable.initialize();&#13;
            dataImportTable.setWorkflow(false);&#13;
&#13;
            dataImportTable.setValue('u_sntablebatch', snTableBatch);&#13;
            dataImportTable.setValue('u_sntablename', snTableName);&#13;
            dataImportTable.setValue('u_record_import_error', e);&#13;
            dataImportTable.insert();&#13;
        }&#13;
&#13;
        //gs.info('*********** Records processed: ' + recordsProcessed + ', batch: ' + snTableBatch);&#13;
    }&#13;
 &#13;
})(inputs, outputs);&#13;
</value>
